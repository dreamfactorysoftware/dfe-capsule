{
    "name":              "dreamfactory/dfe-capsule",
    "description":       "DreamFactory(tm) Enterprise Instance Encapsulation Package",
    "homepage":          "https://www.dreamfactory.com/",
    "keywords":          [
        "dreamfactory",
        "df",
        "dfe",
        "client",
        "api",
        "instance",
        "capsule"
    ],
    "license":           [
        "proprietary"
    ],
    "type":              "library",
    "authors":           [
        {
            "name":  "Jerry Ablan",
            "email": "jerryablan@dreamfactory.com"
        }
    ],
    "repositories":      [
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-database.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-common.git"
        },
        {
            "type": "vcs",
            "url":  "https://github.com/dreamfactorysoftware/dfe-storage.git"
        }
    ],
    "prefer-dist":       false,
    "minimum-stability": "dev",
    "require":           {
        "dreamfactory/dfe-common":   "dev-feature/l52-upgrade as dev-master",
        "dreamfactory/dfe-database": "dev-feature/l52-upgrade as dev-master"
    },
    "require-dev":       {
        "laravel/framework": "~5.2.0",
        "guzzlehttp/guzzle": "~4.0",
        "phpspec/phpspec":   "~2.1",
        "phpunit/phpunit":   "~4.0"
    },
    "autoload":          {
        "psr-4": {
            "DreamFactory\\Enterprise\\Instance\\Capsule\\": "src/"
        }
    },
    "config":            {
        "preferred-install": "dist"
    },
    "extra":             {
        "branch-alias": {
            "dev-develop": "1.0.x-dev"
        }
    }
}
